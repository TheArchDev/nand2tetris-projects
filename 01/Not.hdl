/**
 * Not gate:
 * out = not in
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    Nand(a=in, b=in, out=temp);
    Nand(a=temp, b=in, out=temp2);
    Nand(a=temp2, b=in, out=out);
}